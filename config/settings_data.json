          "background_image_text_FL9Rg6": {
            "type": "background-image-text",
            "settings": {
              "subtitle": "",
              "title": "Configurar tu SnoGo",
              "text": "",
              "button_label": "",
              "button_link": "",
              "image": "shopify://shop_images/5C4A3498_2400x_2fbdd945-c2f1-4a83-9c45-7d6b9d52d10e.webp",
              "alignment": "center",
